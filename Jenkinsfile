pipeline{
    agent any

    stages {
        stage('Build') {
            steps {
                echo "Building"
                echo "Build successful"
            }
        }
        stage("Test") {
            steps {
                echo "Testing"
                echo "Testing successful"
            }
        }
        stage("Deploy") {
            steps {
                echo "Deploying"
                echo "Deployment successful"
            }
        }
    }
}